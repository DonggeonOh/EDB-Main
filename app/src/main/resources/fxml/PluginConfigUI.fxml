<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="477.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.edb.main.UI.PluginConfigUIController">
   <children>
      <ScrollPane prefHeight="369.0" prefWidth="477.0">
         <content>
            <VBox fx:id="configArea" prefHeight="369.0" prefWidth="464.0">
               <children>
                  <AnchorPane prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="targetProgramTableView" prefHeight="155.0" prefWidth="463.0">
                          <columns>
                            <TableColumn fx:id="targetProgramNameColumn" prefWidth="150" text="프로그램 이름" />
                            <TableColumn fx:id="targetProgramPathColumn" prefWidth="250" text="경로" />
                          </columns>
                        </TableView>
                        <Button onAction="#addTargetProgram" layoutX="315.0" layoutY="157.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="58.0" text="추가" />
                        <Button onAction="#deleteTargetProgram" layoutX="398.0" layoutY="157.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="58.0" text="삭제" />
                     </children>
                  </AnchorPane>
                  <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="200.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="targetWebsiteTableView" prefHeight="155.0" prefWidth="463.0">
                           <columns>
                              <TableColumn fx:id="targetWebsiteURLColumn" prefWidth="400" text="웹사이트URL" />
                           </columns>
                        </TableView>
                        <Button onAction="#addTargetWebsite" layoutX="315.0" layoutY="157.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="58.0" text="추가" />
                        <Button onAction="#deleteTargetWebsite" layoutX="398.0" layoutY="157.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="58.0" text="삭제" />
                        <TextField layoutX="11.0" layoutY="158.0" prefHeight="29.0" prefWidth="292.0" />
                     </children>
                  </AnchorPane>
               </children></VBox>
         </content>
      </ScrollPane>
      <AnchorPane fx:id="applyArea" prefHeight="40.0" prefWidth="477.0">
         <children>
            <Button fx:id="applyToServerBtn" onAction="#applyToServer" layoutX="412.0" layoutY="4.0" mnemonicParsing="false" text="서버반영" />
            <Button fx:id="scheduleBtn" onAction="#schedulePlugin" layoutX="320.0" layoutY="4.0" mnemonicParsing="false" text="시간설정" />
            <TextField fx:id="startHourField" layoutX="6.0" layoutY="4.0" prefHeight="20.0" prefWidth="35.0" />
            <TextField fx:id="startMinField" layoutX="80.0" layoutY="4.0" prefHeight="20.0" prefWidth="35.0" />
            <TextField fx:id="endHourField" layoutX="161.0" layoutY="4.0" prefHeight="20.0" prefWidth="35.0" />
            <TextField fx:id="endMinField" layoutX="233.0" layoutY="4.0" prefHeight="20.0" prefWidth="35.0" />
            <Label layoutX="44.0" layoutY="13.0" text="시간" />
            <Label layoutX="115.0" layoutY="13.0" text="분 후" />
            <Label layoutX="199.0" layoutY="13.0" text="시간" />
            <Label layoutX="269.0" layoutY="14.0" text="분 동안" />
         </children></AnchorPane>
   </children>
</VBox>
